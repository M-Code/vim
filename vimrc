set tabstop=4
set shiftwidth=4
set expandtab
set autoindent
set smartindent
set cindent

set number
set ruler
set title
set wildmenu

"colorscheme blue
syntax on

set hlsearch
set incsearch
map <C-K> : e. <CR>
map <F7> :! clear; ../compile.sh <CR>
map <F6> :! clear; ../debug.sh <CR>
map <F5> :! clear; ../run.sh <CR>
